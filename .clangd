CompileFlags:
  Compiler: clang
  Add:
    - -std=gnu23
    - -Wall
    - -Werror
    - -Wpedantic
    - -Winline
    - -Wunused-result
    - -Wconversion
    - -Wsign-conversion
    - -Wformat=2
    - -Wformat-signedness
    - -Wformat-truncation
    - -Wformat-overflow
    - -O3
    - -march=native
    - -mtune=native
    - -fno-plt
    - -fno-exceptions
    - -ffast-math
    - -pipe
    - -flto=full
    - -fvisibility=hidden
    - -ffunction-sections
    - -fdata-sections
    - -Wl,-O1
    - -Wl,--gc-sections
    - -Wl,--sort-common
    - -Wl,--as-needed
    - -Wl,-z,relro
    - -Wl,-z,now
    - -Wl,-z,pack-relative-relocs
    - -ggdb3
    - -DDEBUG
    - -D_FORTIFY_SOURCE=3
    - -fsanitize=undefined
    - -fstack-clash-protection
    - -fcf-protection
    - -ftrapv
    - -fno-omit-frame-pointer
    - -mno-omit-leaf-frame-pointer
    - -Rpass=.*
